    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "8.2.0-next.0",
    "@angular/platform-browser-dynamic": "8.2.0-next.0",
    "@angular/platform-server": "8.2.0-next.0",
    "@angular/router": "8.2.0-next.0",
    "@ngrx/data": "^8.1.0",
    "@ngrx/effects": "^8.1.0",
    "@ngrx/entity": "^8.1.0",
    "@ngrx/router-store": "^8.1.0",
    "@ngrx/schematics": "^8.1.0",
    "@ngrx/store": "^8.1.0",
    "@ngrx/store-devtools": "^8.1.0",
    "@types/lodash.findindex": "^4.6.6",
    "argon2": "^0.24.0",
    "auth0-js": "^9.11.2",
    "body-parser": "^1.19.0",
